<!DOCTYPE html> 
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>UniLearn Blog - Latest Tech Articles</title>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <script src="https://cdn.tailwindcss.com"></script>
    <style>
        body {
            font-family: 'Poppins', sans-serif;
            box-sizing: border-box;
        }
        
        @keyframes fadeInUp {
            0% {
                opacity: 0;
                transform: translateY(30px);
            }
            100% {
                opacity: 1;
                transform: translateY(0);
            }
        }
        
        .fade-in-up {
            animation: fadeInUp 0.8s ease-out forwards;
        }
        
        .gradient-text {
            background: linear-gradient(135deg, #3b82f6, #3b82f6, #06b6d4);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }
        
        .card-hover {
            transition: all 0.4s ease;
            cursor: pointer;
        }
        
        .card-hover:hover {
            transform: translateY(-10px);
            box-shadow: 0 25px 50px rgba(59, 130, 246, 0.15);
        }
        
        .glow-effect {
            box-shadow: 0 0 20px rgba(59, 130, 246, 0.3);
        }
        
        .glass-effect {
            background: rgba(255, 255, 255, 0.05);
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.1);
        }
        
        .search-glow:focus {
            box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.3);
        }
        
        .category-active {
            background: linear-gradient(135deg, #3b82f6, #3b82f6);
            color: white;
        }
        
        .floating-element {
            animation: float 3s ease-in-out infinite;
        }
        
        @keyframes float {
            0%, 100% {
                transform: translateY(0px);
            }
            50% {
                transform: translateY(-10px);
            }
        }
        
        .hidden {
            display: none;
        }
        
        .blog-content {
            line-height: 1.8;
        }

        .blog-content h2 {
            font-size: 1.5rem;
            font-weight: bold;
            margin: 2rem 0 1rem 0;
            color: #1e40af;
        }

        .blog-content h3 {
            font-size: 1.25rem;
            font-weight: bold;
            margin: 1.5rem 0 0.75rem 0;
            color: #3b82f6;
        }

        .blog-content p {
            margin-bottom: 1.5rem;
            color: #374151;
        }

        .blog-content ul {
            margin: 1rem 0;
            padding-left: 2rem;
            color: #374151;
        }

        .blog-content li {
            margin-bottom: 0.5rem;
        }

        .blog-content blockquote {
            border-left: 4px solid #3b82f6;
            padding-left: 1rem;
            margin: 2rem 0;
            font-style: italic;
            color: #6b7280;
            background-color: #f3f4f6;
            padding: 1rem;
            border-radius: 0.5rem;
        }

        .blog-content code {
            background: #f3f4f6;
            padding: 0.25rem 0.5rem;
            border-radius: 0.25rem;
            font-family: 'Courier New', monospace;
            color: #dc2626;
            border: 1px solid #e5e7eb;
        }

        /* Navigation Styles */
        .nav-link { padding: 0.25rem 0.5rem; color: #4b5563; transition: color 0.2s, border-color 0.2s; border-bottom: 2px solid transparent; font-weight: 500; }
        .nav-link:hover { color: #1f2937; border-bottom-color: #3b82f6; }
        .nav-link.active { color: #3b82f6; border-bottom-color: #3b82f6; font-weight: 600; }
        /* Custom Logo Style */
        .header-logo { color: #2563eb; font-weight: 800; }
    </style>
    <script src="/js/darkmode.js"></script>
    <script>
        tailwind.config = {
            darkMode: 'class'
        }
    </script>
    <link rel="stylesheet" href="/css/darkmode-improved.css">
</head>
<body class="font-sans bg-gray-50 dark:bg-gray-700 transition-colors duration-300 dark:bg-gray-900">
    <%- include('../partials/header') %>
    <!-- Navigation -->
    <!-- Blog List View -->
    <div id="blogList" class="pt-4">
        <!-- Header -->
        <section class="py-16 px-4 sm:px-6 lg:px-8 relative overflow-hidden bg-gradient-to-br from-blue-50 via-white to-blue-50 dark:from-black dark:via-gray-900 dark:to-black">
            <div class="absolute top-20 left-10 w-20 h-20 bg-blue-500/10 rounded-full floating-element"></div>
            <div class="absolute top-40 right-20 w-16 h-16 bg-blue-500/10 rounded-full floating-element" style="animation-delay: 1s;"></div>

            <div class="max-w-7xl mx-auto text-center">
                <div class="fade-in-up">
                    <h1 class="text-5xl md:text-6xl font-bold mb-6 leading-tight">
                        <span class="gradient-text">UniLearn Blog</span>
                    </h1> 
                    <p class="text-xl md:text-2xl text-gray-600 dark:text-gray-400 mb-8 max-w-3xl mx-auto leading-relaxed">
                        Explore the latest technology trends, from AI to web development
                    </p> <!-- Title with gradient text -->

                    <!-- Search Bar --> 
                    <div class="max-w-2xl mx-auto mb-12">
                        <div class="relative">
                            <input type="text" id="searchInput" placeholder="Search articles..." class="w-full px-6 py-4 bg-white dark:bg-gray-800 border border-gray-300 dark:border-gray-600 rounded-2xl text-gray-900 dark:text-gray-100 placeholder-gray-400 focus:outline-none focus:border-blue-500 focus:ring-2 focus:ring-blue-200 transition-all duration-300 shadow-sm">
                            <button class="absolute right-3 top-1/2 transform -translate-y-1/2 bg-gradient-to-r from-blue-600 to-blue-600 text-white px-6 py-2 rounded-xl hover:from-blue-700 hover:to-blue-700 transition-all duration-300">
                                <svg class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20">
                                    <path fill-rule="evenodd" d="M8 4a4 4 0 100 8 4 4 0 000-8zM2 8a6 6 0 1110.89 3.476l4.817 4.817a1 1 0 01-1.414 1.414l-4.816-4.816A6 6 0 012 8z" clip-rule="evenodd"/>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <!-- Categories (checkpoint)-->
        <section class="py-12 bg-white dark:bg-gray-800 border-b border-gray-200 dark:border-gray-700">
            <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
                <div class="flex flex-wrap justify-center gap-z 4"> <!-- Category Buttons thêm khoảng cách + căn giữa -->
                    <button class="category-btn category-active px-6 py-3 rounded-full font-semibold transition-all duration-300 hover:scale-105" data-category="all">All</button>
                    <button class="category-btn bg-gray-100 dark:bg-gray-700 text-gray-700 dark:text-gray-300 px-6 py-3 rounded-full font-semibold hover:bg-gray-200 hover:text-gray-900 dark:text-gray-100 transition-all duration-300 hover:scale-105" data-category="ai">AI & Machine Learning</button> <!-- phóng to khi hover bằng hover scale -->
                    <button class="category-btn bg-gray-100 dark:bg-gray-700 text-gray-700 dark:text-gray-300 px-6 py-3 rounded-full font-semibold hover:bg-gray-200 hover:text-gray-900 dark:text-gray-100 transition-all duration-300 hover:scale-105" data-category="web">Web Development</button>
                    <button class="category-btn bg-gray-100 dark:bg-gray-700 text-gray-700 dark:text-gray-300 px-6 py-3 rounded-full font-semibold hover:bg-gray-200 hover:text-gray-900 dark:text-gray-100 transition-all duration-300 hover:scale-105" data-category="mobile">Mobile</button>
                    <button class="category-btn bg-gray-100 dark:bg-gray-700 text-gray-700 dark:text-gray-300 px-6 py-3 rounded-full font-semibold hover:bg-gray-200 hover:text-gray-900 dark:text-gray-100 transition-all duration-300 hover:scale-105" data-category="security">Cybersecurity</button>
                </div>
            </div>
        </section>

        <!-- Articles Grid -->
        <section class="py-20 bg-gray-50 dark:bg-gray-700">
            <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8"> <!-- mx auto cho căn giữa-->
                <div id="articlesGrid" class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-10">
                    <!-- Articles will be populated by JavaScript -->
                </div>

                <div class="text-center mt-16">
                    <button id="loadMoreBtn" class="bg-gradient-to-r from-blue-600 to-blue-600 text-white px-10 py-4 rounded-xl hover:from-blue-700 hover:to-blue-700 transition-all duration-300 font-semibold shadow-lg hover:shadow-xl">
                        Load More Articles
                    </button>
                </div>
            </div>
        </section>
    </div>

    <!-- Single Article View -->
    <div id="singleArticle" class="hidden pt-4">
        <div class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
            <!-- Back Button -->
            <button onclick="showBlogList()" class="flex items-center text-blue-600 hover:text-blue-700 mb-8 transition-colors">
                <svg class="w-5 h-5 mr-2" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M9.707 16.707a1 1 0 01-1.414 0l-6-6a1 1 0 010-1.414l6-6a1 1 0 011.414 1.414L5.414 9H17a1 1 0 110 2H5.414l4.293 4.293a1 1 0 010 1.414z" clip-rule="evenodd"/>
                </svg>
                Back to article list
            </button>
            
            <!-- Article Content -->
            <article id="articleContent">
                <!-- Content will be populated by JavaScript -->
            </article>
            
            <!-- Related Articles -->
            <section class="mt-16 pt-12 border-t border-gray-200 dark:border-gray-700">
                <h3 class="text-2xl font-bold text-gray-900 dark:text-gray-100 mb-8">Related Articles</h3>
                <div id="relatedArticles" class="grid grid-cols-1 md:grid-cols-2 gap-8">
                    <!-- Related articles will be populated by JavaScript -->
                </div>
            </section>
        </div>
    </div>

    <!-- Footer -->
    <script>
        // Authentication check
        const token = localStorage.getItem('token');
        const user = JSON.parse(localStorage.getItem('user') || '{}');

        // Dashboard button is now handled by header.ejs universal script

        // Blog articles will be loaded from API
        let articles = [];
        // Fallback: Empty array if API fails
        const staticArticles = [];

        let currentArticles = [...articles];
        let currentCategory = 'all';
        let articlesPerPage = 6;
        let currentPage = 1;

        // Load blog posts from API
        async function loadBlogPosts() {
            try {
                const response = await fetch((window.location.origin.includes('localhost') ? 'http://localhost:5000' : window.location.origin) + '/api/blog?status=published&limit=20');
                console.log('Blog API response status:', response.status);
                if (response.ok) {
                    const data = await response.json();
                    console.log('Blog API data:', data);

                    // Handle both wrapped and unwrapped response formats
                    const posts = data.data?.posts || data.posts || data || [];

                    if (!Array.isArray(posts)) {
                        console.warn('Blog posts is not an array, using static data');
                        articles = [...staticArticles];
                        return;
                    }

                    // Transform API data to match frontend format
                    articles = posts.map(post => ({
                        id: post.id,
                        title: post.title,
                        excerpt: post.excerpt,
                        category: post.tags && post.tags.length > 0 ? post.tags[0].toLowerCase() : 'general',
                        categoryName: post.tags && post.tags.length > 0 ? post.tags[0] : 'General',
                        author: post.author_name || 'Anonymous',
                        authorInitials: (post.author_name || 'A').split(' ').map(n => n[0]).join('').toUpperCase().substring(0, 2),
                        date: post.created_at ? (post.created_at.seconds ? new Date(post.created_at.seconds * 1000).toLocaleDateString('vi-VN') : new Date(post.created_at).toLocaleDateString('vi-VN')) : new Date().toLocaleDateString('vi-VN'),
                        readTime: Math.ceil((post.content?.length || 1000) / 200) + ' min read',
                        emoji: getEmojiByCategory(post.tags && post.tags.length > 0 ? post.tags[0].toLowerCase() : 'general'),
                        gradient: getGradientByCategory(post.tags && post.tags.length > 0 ? post.tags[0].toLowerCase() : 'general'),
                        content: post.content || '',
                        featured_image: post.featured_image || null
                    }));
                } else {
                    console.warn('Failed to load blog posts from API, using static data');
                    articles = [...staticArticles];
                }
            } catch (error) {
                console.error('Error loading blog posts:', error);
                articles = [...staticArticles];
            }

            // Update currentArticles after loading
            currentArticles = [...articles];
        }

        // Helper functions for mapping categories
        function getEmojiByCategory(category) {
            const emojiMap = {
                'ai': '🤖',
                'machine learning': '🧠',
                'web': '💻',
                'web development': '💻',
                'mobile': '📱',
                'security': '🔒',
                'cybersecurity': '🔒',
                'blockchain': '⛓️',
                'general': '📝'
            };
            return emojiMap[category] || '📝';
        }

        function getGradientByCategory(category) {
            const gradientMap = {
                'ai': 'from-blue-500 to-blue-600',
                'machine learning': 'from-cyan-500 to-blue-600',
                'web': 'from-green-500 to-blue-600',
                'web development': 'from-green-500 to-blue-600',
                'mobile': 'from-blue-500 to-blue-600',
                'security': 'from-red-500 to-pink-600',
                'cybersecurity': 'from-red-500 to-pink-600',
                'blockchain': 'from-yellow-500 to-orange-600',
                'general': 'from-gray-500 to-gray-600'
            };
            return gradientMap[category] || 'from-gray-500 to-gray-600';
        }

        // Initialize the blog
        async function initBlog() {
            await loadBlogPosts();
            renderArticles();
            setupEventListeners();
        }

        // Render articles grid
        function renderArticles() {
            const grid = document.getElementById('articlesGrid');
            const startIndex = (currentPage - 1) * articlesPerPage;
            const endIndex = startIndex + articlesPerPage;
            const articlesToShow = currentArticles.slice(0, endIndex);
            
            grid.innerHTML = articlesToShow.map(article => `
                <article class="card-hover bg-white dark:bg-gray-800 rounded-2xl overflow-hidden shadow-lg border border-gray-200 dark:border-gray-700 hover:shadow-xl" onclick="showArticle('${article.id}')">
                    <div class="h-48 bg-gradient-to-br ${article.gradient} flex items-center justify-center relative overflow-hidden">
                        ${article.featured_image ?
                            `<img src="${article.featured_image}" alt="${article.title}" class="w-full h-full object-cover absolute inset-0">` :
                            `<span class="text-6xl relative z-10">${article.emoji}</span>`
                        }
                    </div>
                    <div class="p-8">
                        <div class="flex items-center mb-4">
                            <span class="bg-${article.category === 'ai' ? 'blue' : article.category === 'web' ? 'green' : article.category === 'mobile' ? 'purple' : 'red'}-100 text-${article.category === 'ai' ? 'blue' : article.category === 'web' ? 'green' : article.category === 'mobile' ? 'purple' : 'red'}-700 text-xs font-semibold px-3 py-1 rounded-full border border-${article.category === 'ai' ? 'blue' : article.category === 'web' ? 'green' : article.category === 'mobile' ? 'purple' : 'red'}-200 mr-3">${article.categoryName}</span>
                            <span class="text-gray-500 text-sm">${article.date}</span>
                        </div>
                        <h3 class="text-xl font-bold text-gray-900 dark:text-gray-100 mb-3 leading-tight">${article.title}</h3>
                        <p class="text-gray-600 dark:text-gray-400 mb-6 leading-relaxed">${article.excerpt}</p>
                        <div class="flex items-center justify-between">
                            <div class="flex items-center">
                                <div class="w-8 h-8 bg-gradient-to-r ${article.gradient} rounded-full flex items-center justify-center mr-3">
                                    <span class="text-white text-sm font-bold">${article.authorInitials}</span>
                                </div>
                                <span class="text-gray-700 dark:text-gray-300 text-sm">${article.author}</span>
                            </div>
                            <span class="text-gray-500 text-sm">${article.readTime}</span>
                        </div>
                    </div>
                </article>
            `).join('');

            // Show/hide load more button
            const loadMoreBtn = document.getElementById('loadMoreBtn');
            if (endIndex >= currentArticles.length) {
                loadMoreBtn.style.display = 'none';
            } else {
                loadMoreBtn.style.display = 'block';
            }
        }

        // Show single article
        async function showArticle(id) {
            try {
                // Try to fetch from API first
                const response = await fetch(`${window.location.origin.includes('localhost') ? 'http://localhost:5000' : window.location.origin}/api/blog/${id}`);
                let article;

                if (response.ok) {
                    const apiPost = await response.json();
                    article = {
                        id: apiPost.id,
                        title: apiPost.title,
                        excerpt: apiPost.excerpt,
                        category: apiPost.tags && apiPost.tags.length > 0 ? apiPost.tags[0].toLowerCase() : 'general',
                        categoryName: apiPost.tags && apiPost.tags.length > 0 ? apiPost.tags[0] : 'General',
                        author: apiPost.author_name || 'Anonymous',
                        authorInitials: (apiPost.author_name || 'A').split(' ').map(n => n[0]).join('').toUpperCase().substring(0, 2),
                        date: apiPost.created_at ? (apiPost.created_at.seconds ? new Date(apiPost.created_at.seconds * 1000).toLocaleDateString('vi-VN') : new Date(apiPost.created_at).toLocaleDateString('vi-VN')) : new Date().toLocaleDateString('vi-VN'),
                        readTime: Math.ceil((apiPost.content?.length || 1000) / 200) + ' min read',
                        emoji: getEmojiByCategory(apiPost.tags && apiPost.tags.length > 0 ? apiPost.tags[0].toLowerCase() : 'general'),
                        gradient: getGradientByCategory(apiPost.tags && apiPost.tags.length > 0 ? apiPost.tags[0].toLowerCase() : 'general'),
                        content: apiPost.content || '',
                        featured_image: apiPost.featured_image || null
                    };
                } else {
                    // Fallback to local articles
                    article = articles.find(a => a.id === id);
                }

                if (!article) return;

                document.getElementById('blogList').classList.add('hidden');
                document.getElementById('singleArticle').classList.remove('hidden');

                // Create full article content with proper styling
                const fullContent = `
                    <h1 class="text-4xl font-bold text-gray-900 dark:text-gray-100 mb-6">${article.title}</h1>
                    <div class="flex items-center mb-8 text-gray-600 dark:text-gray-400">
                        <span class="bg-blue-100 text-blue-700 text-sm font-semibold px-3 py-1 rounded-full border border-blue-200 mr-4">${article.categoryName}</span>
                        <span class="mr-4">${article.date}</span>
                        <span class="mr-4">•</span>
                        <span class="mr-4">${article.readTime}</span>
                        <span>•</span>
                        <span class="ml-4">${article.author}</span>
                    </div>

                    <div class="h-64 bg-gradient-to-br ${article.gradient} rounded-2xl flex items-center justify-center mb-8 relative overflow-hidden shadow-lg">
                        ${article.featured_image ?
                            `<img src="${article.featured_image}" alt="${article.title}" class="w-full h-full object-cover absolute inset-0 rounded-2xl">` :
                            `<span class="text-8xl relative z-10">${article.emoji}</span>`
                        }
                    </div>

                    <div class="blog-content">
                        ${article.content}
                    </div>
                `;

                document.getElementById('articleContent').innerHTML = fullContent;

                // Render related articles
                const relatedArticles = articles
                    .filter(a => a.id !== id && a.category === article.category)
                    .slice(0, 2);

                document.getElementById('relatedArticles').innerHTML = relatedArticles.map(related => `
                    <article class="card-hover bg-white dark:bg-gray-800 rounded-xl overflow-hidden shadow-lg border border-gray-200 dark:border-gray-700 hover:shadow-xl" onclick="showArticle('${related.id}')">
                        <div class="h-32 bg-gradient-to-br ${related.gradient} flex items-center justify-center relative overflow-hidden">
                            ${related.featured_image ?
                                `<img src="${related.featured_image}" alt="${related.title}" class="w-full h-full object-cover absolute inset-0">` :
                                `<span class="text-4xl relative z-10">${related.emoji}</span>`
                            }
                        </div>
                        <div class="p-6">
                            <h4 class="text-lg font-bold text-gray-900 dark:text-gray-100 mb-2">${related.title}</h4>
                            <p class="text-gray-600 dark:text-gray-400 text-sm mb-4">${related.excerpt.substring(0, 100)}...</p>
                            <div class="flex items-center justify-between text-sm">
                                <span class="text-gray-700 dark:text-gray-300">${related.author}</span>
                                <span class="text-gray-500">${related.readTime}</span>
                            </div>
                        </div>
                    </article>
                `).join('');

                // Scroll to top
                window.scrollTo(0, 0);
            } catch (error) {
                console.error('Error showing article:', error);
            }
        }

        // Show blog list
        function showBlogList() {
            document.getElementById('singleArticle').classList.add('hidden');
            document.getElementById('blogList').classList.remove('hidden');
            window.scrollTo(0, 0);
        }

        // Filter articles by category
        function filterByCategory(category) {
            currentCategory = category;
            currentPage = 1;
            
            if (category === 'all') {
                currentArticles = [...articles];
            } else {
                currentArticles = articles.filter(article => article.category === category);
            }
            
            renderArticles();
            
            // Update active category button
            document.querySelectorAll('.category-btn').forEach(btn => {
                btn.classList.remove('category-active');
                btn.classList.add('bg-gray-100 dark:bg-gray-700', 'text-gray-700 dark:text-gray-300');
            });

            const activeBtn = document.querySelector(`[data-category="${category}"]`);
            activeBtn.classList.add('category-active');
            activeBtn.classList.remove('bg-gray-100 dark:bg-gray-700', 'text-gray-700 dark:text-gray-300');
        }

        // Search articles
        function searchArticles(query) {
            if (!query.trim()) {
                currentArticles = [...articles];
            } else {
                currentArticles = articles.filter(article => 
                    article.title.toLowerCase().includes(query.toLowerCase()) ||
                    article.excerpt.toLowerCase().includes(query.toLowerCase()) ||
                    article.categoryName.toLowerCase().includes(query.toLowerCase())
                );
            }
            currentPage = 1;
            renderArticles();
        }

        // Setup event listeners
        function setupEventListeners() {
            // Category buttons
            document.querySelectorAll('.category-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    filterByCategory(btn.dataset.category);
                });
            });

            // Search
            const searchInput = document.getElementById('searchInput');
            searchInput.addEventListener('input', (e) => {
                searchArticles(e.target.value);
            });

            // Load more button
            document.getElementById('loadMoreBtn').addEventListener('click', () => {
                currentPage++;
                renderArticles();
            });

            // Logout functionality - header handles the button, just ensure it works
            const logoutBtn = document.getElementById('logout-button');
            const mobileLogoutBtn = document.getElementById('mobile-logout-button');

            if (logoutBtn) {
                logoutBtn.addEventListener('click', () => {
                    localStorage.clear();
                    window.location.href = '/login';
                });
            }

            if (mobileLogoutBtn) {
                mobileLogoutBtn.addEventListener('click', () => {
                    localStorage.clear();
                    window.location.href = '/login';
                });
            }
        }

        // Initialize when page loads
        document.addEventListener('DOMContentLoaded', initBlog);

        // Make functions global for onclick handlers
        window.showArticle = showArticle;
        window.showBlogList = showBlogList;
    </script>
